{
  "$schema": "https://dl.viam.dev/module.schema.json",
  "module_id": "joyce:pyzbar",
  "visibility": "public",
  "url": "https://github.com/loopDelicious/viam-qrcode",
  "description": "Detect QR codes on a Viam camera using Pyzbar and OpenCV",
  "models": [
    {
      "api": "rdk:service:vision",
      "model": "joyce:vision:pyzbar"
    }
  ],
  "build": {
    "setup": "./setup.sh",
    "build": "./build.sh",
    "path": "dist/archive.tar.gz",
    "arch": ["linux/amd64", "linux/arm64", "darwin/arm64"]
  },
  "entrypoint": "dist/main/main"
}